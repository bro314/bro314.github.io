<SCRIPT language="JavaScript">
<!--

var x,y
var flag=0
var imgnum=6
var xpos=new Array(-150,-150,-150,-150,-150,-150)
var ypos=new Array(-150,-150,-150,-150,-150,-150)
var step=new Array(10,50,50,50,50,50)

if (document.layers){
  document.captureEvents(Event.MOUSEMOVE);
}

document.onmousemove = handlerMM;

function handlerMM(e){
  x = (document.layers) ? e.pageX : document.body.scrollLeft + event.clientX
  y = (document.layers) ? e.pageY : document.body.scrollTop + event.clientY
  flag=1
}

function makesnake() {
  if (flag==1 && document.all) {
    for (i=imgnum-1; i>=1; i--) {
      xpos[i]=xpos[i-1]+step[i]
      ypos[i]=ypos[i-1]
    }
    xpos[0]=x+step[0]
    ypos[0]=y
  
    for (i=0; i<imgnum-1; i++) {
      var thisspan = eval("span"+(i)+".style")
      thisspan.posLeft=xpos[i]
      thisspan.posTop=ypos[i]
    }
  }
  
  else if (flag==1 && document.layers) {
    for (i=imgnum-1; i>=1; i--) {
      xpos[i]=xpos[i-1]+step[i]
      ypos[i]=ypos[i-1]
    }
    xpos[0]=x+step[0]
    ypos[0]=y
  
    for (i=0; i<imgnum-1; i++) {
      var thisspan = eval("document.span"+i)
      thisspan.left=xpos[i]
      thisspan.top=ypos[i]
    }
  }
  
  setTimeout("makesnake()",30)
}

//-->
</SCRIPT>

